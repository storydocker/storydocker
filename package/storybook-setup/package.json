{
  "name": "storydocker-storybook",
  "version": "0.0.9",
  "type": "module",
  "description": "The default multi-framework Storybook setup used by StoryDocker",
  "keywords": [
    "storybook",
    "storybookjs",
    "storydocker",
    "testing-library"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "npm run storybook",
    "start": "npm run dev",
    "dev-vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 2001",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --url http://localhost:2001  --coverage --verbose",
    "chromatic": "npx chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "test": "vitest",
    "coverage": "vitest run --coverage --reporter=verbose"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^7.0.23",
    "@storybook/addon-coverage": "^0.0.8",
    "@storybook/addon-essentials": "^7.0.23",
    "@storybook/addon-interactions": "^7.0.23",
    "@storybook/addon-links": "^7.0.23",
    "@storybook/addon-mdx-gfm": "^7.0.23",
    "@storybook/blocks": "^7.0.23",
    "@storybook/html": "^7.0.23",
    "@storybook/html-vite": "^7.0.23",
    "@storybook/jest": "^0.1.0",
    "@storybook/preact": "^7.0.23",
    "@storybook/preact-vite": "^7.0.23",
    "@storybook/react": "^7.0.23",
    "@storybook/react-vite": "^7.0.23",
    "@storybook/svelte": "^7.0.23",
    "@storybook/svelte-vite": "^7.0.23",
    "@storybook/test-runner": "^0.11.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.0.23",
    "@storybook/vue3-vite": "^7.0.23",
    "@storybook/web-components": "^7.0.23",
    "@storybook/web-components-vite": "^7.0.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.23",
    "storydocker-cli": "^0.0.7"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@tsconfig/svelte": "^4.0.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "lit": "^2.7.5",
    "preact": "^10.15.1",
    "prop-types": "^15.8.1",
    "svelte": "^3.59.2",
    "svelte-check": "^3.4.4",
    "vite": "^4.3.9",
    "vue": "^3.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/storydocker/storydocker.git"
  },
  "homepage": "https://github.com/storydocker/storydocker",
  "author": {
    "name": "scottnath",
    "url": "https://scottnath.com"
  }
}
