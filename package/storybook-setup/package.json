{
  "name": "storydocker-storybook",
  "version": "0.0.9",
  "type": "module",
  "description": "The default multi-framework Storybook setup used by StoryDocker",
  "keywords": [
    "storybook",
    "storybookjs",
    "storydocker",
    "testing-library"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "npm run storybook",
    "start": "npm run dev",
    "dev-vite": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 2001",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook --url http://localhost:2001  --coverage --verbose",
    "test": "vitest",
    "coverage": "vitest run --coverage --reporter=verbose"
  },
  "dependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@guidepup/virtual-screen-reader": "^0.24.1",
    "@storybook/addon-a11y": "^8.1.2",
    "@storybook/addon-coverage": "^1.0.3",
    "@storybook/addon-essentials": "^8.1.2",
    "@storybook/addon-interactions": "^8.1.2",
    "@storybook/addon-links": "^8.1.2",
    "@storybook/addon-mdx-gfm": "^8.1.2",
    "@storybook/blocks": "^8.1.1",
    "@storybook/html": "^8.1.2",
    "@storybook/html-vite": "^8.1.2",
    "@storybook/preact": "^8.1.2",
    "@storybook/preact-vite": "^8.1.2",
    "@storybook/react": "^8.1.2",
    "@storybook/react-vite": "^8.1.2",
    "@storybook/svelte": "^8.1.2",
    "@storybook/svelte-vite": "^8.1.2",
    "@storybook/test": "^8.1.1",
    "@storybook/test-runner": "^0.18.1",
    "@storybook/vue3": "^8.1.2",
    "@storybook/vue3-vite": "^8.1.2",
    "@storybook/web-components": "^8.1.2",
    "@storybook/web-components-vite": "^8.1.2",
    "@vitest/coverage-istanbul": "^1.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shadow-dom-testing-library": "^1.11.2",
    "storybook": "^8.1.2",
    "storydocker-cli": "^0.0.7"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@tsconfig/svelte": "^5.0.4",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "lit": "^3.1.3",
    "preact": "^10.22.0",
    "prop-types": "^15.8.1",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "vite": "^5.2.11",
    "vue": "^3.4.27"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storydocker/storydocker.git"
  },
  "homepage": "https://github.com/storydocker/storydocker",
  "author": {
    "name": "scottnath",
    "url": "https://scottnath.com"
  },
  "overrides": {
    "@vitejs/plugin-vue": "^5.0.4",
    "magic-string": "^0.30.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vitest": "^1.3.1"
  }
}
